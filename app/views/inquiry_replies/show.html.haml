%p
  %b Message:
  = @inquiry_reply.message
%p
  %b From:
  - realtor = @inquiry_reply.user
  = realtor.name

=link_to 'Back', inquiry_path(@inquiry_reply.inquiry_id), { :method => :get, :class => 'button-xs' }
- if current_user.admin? or @inquiry_reply.user.id == current_user.id
  = link_to 'Edit', edit_inquiry_reply_path(@inquiry_reply), :class => 'button-xs'
  = link_to 'Destroy', inquiry_reply_url(@inquiry_reply), { :method => :delete, :class => 'button-xs' }
